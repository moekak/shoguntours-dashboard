{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "quotes": ["error", "single", { "avoidEscape": true }],
        "jsx-a11y/aria-proptypes": "off"
    }
}
